# 环境配置示例文件
# 复制此文件为 .env.development 或 .env.production 并修改对应配置

# 开发环境配置
NODE_ENV=development
VUE_APP_ENV=development

# API配置
VUE_APP_API_URL=http://localhost:8000/api/v1
VUE_APP_API_TIMEOUT=30000

# 应用配置
VUE_APP_TITLE=用户反馈分析系统
VUE_APP_VERSION=1.0.0

# 开发工具配置
VUE_APP_ENABLE_DEVTOOLS=true
VUE_APP_ENABLE_MOCK=false 